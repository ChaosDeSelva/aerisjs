{
  "name": "Aeris.js",
  "description": "A javscript library for maps and weather.",
  "version": "0.1.0",
  "url": "http://aerisjs.com",
  "logo": "themes/public/images/aeris_logo.png",
  "helpers": [
    "scripts/helpers/common.js",
    "themes/public/helpers/helpers.js",
    "themes/public/helpers/typeutil.js",
    "themes/api/helpers/helpers.js"
  ],
  "config": {
    "version": "0.1.1",
    "cdnPath": "//js.aerisapi.com/aerisjs/v{VERSION}/{PACKAGE}.min.js",
    "cndPathLatest": "//js.aerisapi.com/aerisjs.js",
    "githubUrl": "https://github.com/hamweather/aerisjs",
    "aerisApiUrl": "http://www.hamweather.com/support/documentation/aeris",
    "demoPath": "http://uat.hamweather.com/eschwartz/examples",
    "assetPath": "/docs/assets",
    "apiDocsPath": "/docs/api",
    "publicDocsPath": "/docs/public"
  },
  "options": {
    "outdir": "./api",
    "linkNatives": true,
    "paths": "../lib/aeris",
    "themedir": "themes/api"
  }
}