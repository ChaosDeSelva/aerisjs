<h4 id="{{name}}" class="classTitle"><small>{{namespace}}.</small>{{shortName this}}</h4>

<ul class="classDocs">
  <li>
    {{#eachType name}}
        <a href="#{{name}}" class="ref">{{name}}</a>
    {{/eachType}}
  </li>
  {{#if parent}}
    <li>
      extends
      {{#eachType extends}}
        <a href="#{{name}}" class="ref">{{name}}</a>
      {{/eachType}}
    </li>
  {{/if}}
</ul>

{{#if description }}
  {{>eachtypeintext description}}
{{/if}}


<h5 id="{{name}}-constructor">constructor</h5>

<code class="example">new {{name}}({{#joinEach params ', '}}{{name}}{{/joinEach}});</code>

{{#if params}}
  <table>
    <thead>
    <tr>
      <td>Parameter</td>
      <td>Type</td>
      <td>Description</td>
    </tr>
    </thead>
    {{#each params }}
      <tr>
        <td><code>{{name}}</code></td>
        <td>{{>eachtypelink type}}</td>
        <td>{{description}}</td>
      </tr>
    {{/each}}
  </table>
{{/if}}

{{#if methods}}

  <h5 id="{{name}}-methods">methods</h5>

  <table class="methods">
    <thead>
    <tr>
      <td>Method</td>
      <td>Return value</td>
      <td>Description</td>
    </tr>
    </thead>
    {{#each methods}}
      <tr>
        <td>
          <code>
            <strong>{{name}}</strong>({{#joinEach params ', '}}{{name}}:{{>eachtypelink type}}{{/joinEach}})
          </code>
        </td>
        <td>
          {{#if return}}
            {{>eachtypelink return.type}}
          {{/if}}
        </td>
        <td>
          {{#if description}}
            {{>eachtypeintext description}}
          {{/if}}
          {{#if return.description}}
            Returns: {{>eachtypeintext return.description}}
          {{/if}}
        </td>
      </tr>
    {{/each}}
  </table>
{{/if}}