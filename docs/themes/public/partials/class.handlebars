<h4 id="{{name}}">{{shortName this}}</h4>

<ul class="classDocs">
  <li>
    {{>eachtypelink name}}
  </li>
  {{#if parent}}
    <li>
      extends {{>eachtypelink extends }}
    </li>
  {{/if}}
</ul>

{{#if description }}
  {{#eachTypeInText description }}
    {{>typelink this}}
  {{/eachTypeInText}}
{{/if}}


<h5 id="{{name}}-constructor">constructor</h5>

<code class="example">new {{name}}({{#joinEach params ', '}}{{name}}{{/joinEach}});</code>

{{#if params}}
  <table>
    <thead>
    <tr>
      <td>Parameter</td>
      <td>Type</td>
      <td>Description</td>
    </tr>
    </thead>
    {{#each params }}
      <tr>
        <td><code>{{name}}</code></td>
        <td>{{>eachtypelink type}}</td>
        <td>{{description}}</td>
      </tr>
    {{/each}}
  </table>
{{/if}}

{{#if methods}}

  <h5 id="{{name}}-methods">methods</h5>

  <table>
    <thead>
    <tr>
      <td>Method</td>
      <td>Return value</td>
      <td>Description</td>
    </tr>
    </thead>
    {{#each methods}}
      <tr>
        <td>
          <code>
            {{name}}({{#joinEach params ', '}}{{name}}:{{>eachtypelink type}}{{/joinEach}})
          </code>
        </td>
        <td>
          {{#if return}}
            {{>eachtypelink return.type}}
          {{else}}
            none
          {{/if}}
        </td>
        <td></td>
      </tr>
    {{/each}}
  </table>
{{/if}}